{
  "name": "electron-system-idle-time",
  "version": "0.0.1-alpha",
  "description": "Example of electron app",
  "scripts": {
    "dev": "yarn start-renderer-dev",
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-main": "webpack --env.NODE_ENV=production --env.DEBUG_PROD=true --config webpack.main.prod.js --colors --progress --profile",
    "package": "yarn build && electron-builder --dir",
    "build-renderer": "webpack --env.NODE_ENV=production --config webpack.renderer.prod.js --colors --progress --profile",
    "start-renderer-dev": "webpack-dev-server --env.NODE_ENV=development --config webpack.renderer.dev.js --colors",
    "start-main-dev": "electron -r '@babel/register' app/main",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": {
    "name": "Webpal",
    "url": "https://github.com/web-pal"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-pal/chronos-timetracker.git"
  },
  "build": {
    "productName": "Electron-system-idle-time",
    "appId": "com.web-pal.electron-system-idle-time",
    "copyright": "Copyright Â© 2018 Webpal",
    "files": [
      "dist/",
      "node_modules/",
      "main.prod.js",
      "main.prod.js.map"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tool"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Clock"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github"
    }
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@babel/runtime": "7.0.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-hot-loader": "4.3.6",
    "styled-components": "3.4.5",
    "redux": "4.0.0",
    "redux-saga": "1.0.0-beta.2",
    "reselect": "3.0.1",
    "moment": "2.22.2",
    "nanotimer": "Krb686/nanotimer"
  },
  "devDependencies": {
    "concurrently": "4.0.1",
    "electron": "3.0.0-beta.8",
    "electron-builder": "20.28.4",
    "electron-debug": "2.0.0",
    "electron-devtools-installer": "2.2.4",
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-do-expressions": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-flow": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.2",
    "babel-plugin-styled-components": "1.6.3",
    "babel-plugin-redux-saga": "1.0.0-beta.2",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "css-loader": "1.0.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "style-loader": "0.23.0",
    "webpack": "4.17.2",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.7",
    "webpack-merge": "4.1.4",
    "webpack-bundle-analyzer": "2.13.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "flow-bin": "0.80.0"
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=5.x",
    "yarn": ">=1.8.0"
  }
}
